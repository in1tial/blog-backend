<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.article.ArticleDao">
    <select id="findArticleList" resultType="com.blog.article.Article">
        select * from t_article
    </select>

    <select id="findArticleById" resultType="com.blog.article.Article">
        select * from t_article where ARTICLE_ID=#{id}
    </select>
</mapper>